<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.AnaSatis">

    <GridView
        android:id="@+id/ana_satis_fragment_grid_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="8dp"
        android:numColumns="2"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="50dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/ana_fragmetn_button_1"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="1" />

        <Button
            android:id="@+id/ana_fragmetn_button_2"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="2" />

        <Button
            android:id="@+id/ana_fragmetn_button_3"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="3" />

        <Button
            android:id="@+id/ana_fragmetn_button_4"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="4" />

        <Button
            android:id="@+id/ana_fragmetn_button_5"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="5" />

        <Button
            android:id="@+id/ana_fragmetn_button_6"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="6" />

        <Button
            android:id="@+id/ana_fragmetn_button_7"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="7" />

        <Button
            android:id="@+id/ana_fragmetn_button_8"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="8" />

        <Button
            android:id="@+id/ana_fragmetn_button_9"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="9" />

        <Button
            android:id="@+id/ana_fragmetn_button_0"
            android:layout_width="match_parent"
            android:layout_height="40sp"
            android:text="0" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="312sp"
        android:layout_height="52sp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ana_satis_fragment_grid_view">

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Name" />

        <Button
            android:id="@+id/button12"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:text="Button" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
